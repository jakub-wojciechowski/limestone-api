{
  "name": "limestone-api-v2-beta",
  "version": "0.2.3",
  "description": "An api to access trusted token pricing data secured on Arweave and protected by provider's collateral. ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
